create database schema1;
use schema1;
CREATE TABLE table_name1 (
    username VARCHAR(255),
    email VARCHAR(255),
    password VARCHAR(255),  -- Increased size to accommodate hashed passwords
    cpassword VARCHAR(255)  -- Same as above
);
SELECT * FROM table_name1;
truncate table_name1;
CREATE TABLE user_details12 (
    id BIGINT AUTO_INCREMENT PRIMARY KEY,
    firstname VARCHAR(255) NOT NULL,
    lastname VARCHAR(255),
    fathername VARCHAR(255),
    email VARCHAR(255) UNIQUE NOT NULL,
    contact VARCHAR(255),
    course VARCHAR(255),
    university VARCHAR(255),
    passoutyear INT NOT NULL,
    marks FLOAT(53) NOT NULL, -- Example: 99.99
    country VARCHAR(255),
    state VARCHAR(255),
    district VARCHAR(255),
    city VARCHAR(255),
    pincode VARCHAR(6) NOT NULL,
    skill1 VARCHAR(255),
    skill2 VARCHAR(255),
    skill3 VARCHAR(255),
    skill1_level VARCHAR(255),
    skill2_level VARCHAR(255),
    skill3_level VARCHAR(255)
);
SELECT * FROM user_details12;
